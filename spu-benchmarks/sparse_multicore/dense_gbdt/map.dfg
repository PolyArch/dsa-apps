Input32: feat_val
Input32: split_thres
Input32: child1_offset
Input32: child2_offset
Input32: node_id

cond = ICmp32(feat_val, split_thres)

cur_c1_index = Acc32(1,Mul32(cond,4))
cur_c2_index = Acc32(1,Mul32(Not(cond),4))

child1_addr = Add32(child1_offset, cur_c1_index)
child2_addr = Add32(child2_offset, cur_c2_index)

offset = Select32(child1_addr, child2_addr, cond)
id = node_id

Output32: offset
Output32: id
